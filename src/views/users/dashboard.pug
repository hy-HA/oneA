extends ../base
include ../mixins/video
include ../mixins/bbs



block content
    div.profile__question
        h1="달성현황 Dashboard"
        div.profile__question
            h2="올해의 목표는?"
            div     
                h1=user.yeargoal
        div.profile__question
            h2="이번달의 목표는?"
            div     
                h1=user.monthgoal
        div.profile__question     
            each bbs in bbses
                +bbs(bbs)
            else
                span.empty__message No videos found 